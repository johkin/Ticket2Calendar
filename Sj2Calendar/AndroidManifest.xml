<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="se.acrend.sj2cal" android:versionName="0.9.0"
	android:versionCode="090" android:installLocation="auto">
	<application android:label="@string/app_name" android:icon="@drawable/sj2cal"
		android:debuggable="true">
		<activity android:label="@string/app_name" android:name=".activity.Home">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>

		<receiver android:name="SmsReceiver">
			<intent-filter android:priority="1000">
				<action android:name="android.provider.Telephony.SMS_RECEIVED" />
			</intent-filter>
		</receiver>

	</application>
	<uses-permission android:name="android.permission.READ_CALENDAR" />
	<uses-permission android:name="android.permission.WRITE_CALENDAR" />
	<uses-permission android:name="android.permission.RECEIVE_SMS" />
	<uses-sdk android:minSdkVersion="6" android:targetSdkVersion="8"></uses-sdk>
</manifest> 